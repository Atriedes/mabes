<form action="#" class="col-sm-9 col-sm-offset-1 form-horizontal page-form">
	{% if isError or isSuccess %}
		{% include 'Components/_message.twig' %}
	{% endif %}
	<input type="hidden" value="{{ csrf_token }}" name="{{ csrf_key }}">
	<div class="form-box">
		<div class="form-group">
			<label for="transferAmount" class="col-sm-4">Jumlah Transfer</label>
			<div class="col-sm-8">
				<input type="text" id="transferAmount" name="transferAmount" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-12 text-center">
				<h4>Transfer Dari</h4>
			</div>
		</div>
		<div class="form-group">
			<label for="accNumber" class="col-sm-4">Nomor Login MabesForex</label>
			<div class="col-sm-8">
				<input type="text" id="accNumber" name="accNumber" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="accName" class="col-sm-4">Nama Akun MabesForex</label>
			<div class="col-sm-8">
				<input type="text" id="accName" name="accName" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="phonePassword" class="col-sm-4">Phone Password</label>
			<div class="col-sm-8">
				<input type="text" id="phonePassword" name="phonePassword" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="email" class="col-sm-4">E-Mail</label>
			<div class="col-sm-8">
				<input type="text" id="email" name="email" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="phoneNumber" class="col-sm-4">Nomor Handphone (ex. 081654453432)</label>
			<div class="col-sm-8">
				<input type="text" id="phoneNumber" name="phoneNumber" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-12 text-center">
				<h4>Transfer Ke</h4>
			</div>
		</div>
		<div class="form-group">
			<label for="accNumber" class="col-sm-4">Nomor Login MabesForex</label>
			<div class="col-sm-8">
				<input type="text" id="accNumber" name="accNumber" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="accName" class="col-sm-4">Nama Akun MabesForex</label>
			<div class="col-sm-8">
				<input type="text" id="accName" name="accName" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="captcha" class="col-sm-12 text-center">Ketik ulang teks berikut</label>
			<div class="col-sm-8 col-sm-offset-2 captcha-img">
				<img src="http://placehold.it/128x80" class="center-block" alt="">
			</div>
			<div class="col-sm-8 col-sm-offset-2">
				<input type="text" id="captcha" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-6 col-sm-offset-3">
				<button type="submit" class="btn btn-primary btn-block">
					Kirim
				</button>
			</div>
		</div>
	</div>
</form>
