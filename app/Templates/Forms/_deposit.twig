<form action="#" class="col-sm-9 col-sm-offset-1 form-horizontal page-form">
	<div class="form-box">
		<div class="form-group">
			<label for="depositAmount" class="col-sm-4">Jumlah Deposit</label>
			<div class="col-sm-8">
				<input type="text" id="depositAmount" name="depositAmount" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="transferAmount" class="col-sm-4">Total Transfer</label>
			<div class="col-sm-8">
				<input type="text" id="transferAmount" name="transferAmount" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="loginNumber" class="col-sm-4">Nomor Login MabesForex</label>
			<div class="col-sm-8">
				<input type="text" id="loginNumber" name="loginNumber" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="accountName" class="col-sm-4">Nama Akun MabesForex</label>
			<div class="col-sm-8">
				<input type="text" id="accountName" name="accountName" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="transferTo" class="col-sm-4">Transfer Ke</label>
			<div class="col-sm-8">
				<select name="transferTo" id="transferTo" class="form-control">
					<option value="1">1</option>
					<option value="2">2</option>
					<option value="3">3</option>
				</select>
			</div>
		</div>
		<div class="form-group">
			<label for="transferFrom" class="col-sm-4">Transfer Dari</label>
			<div class="col-sm-8">
				<select name="transferFrom" id="transferFrom" class="form-control">
					<option value="1">1</option>
					<option value="2">2</option>
					<option value="3">3</option>
				</select>
			</div>
		</div>
		<div class="form-group">
			<label for="accNumber" class="col-sm-4">Nomor Rekening</label>
			<div class="col-sm-8">
				<input type="text" id="accNumber" name="accNumber" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="accHolder" class="col-sm-4">Nama Pemilik Rekening</label>
			<div class="col-sm-8">
				<input type="text" id="accHolder" name="accHolder" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="email" class="col-sm-4">E-Mail</label>
			<div class="col-sm-8">
				<input type="text" id="email" name="email" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="phoneNumber" class="col-sm-4">Nomor Handphone (ex. 081654453432)</label>
			<div class="col-sm-8">
				<input type="text" id="phoneNumber" name="phoneNumber" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="transferFile" class="col-sm-4">File Bukti Transfer</label>
			<div class="col-sm-8">
				<input type="file" id="transferFile" name="transferFile" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<label for="captcha" class="col-sm-12 text-center">Ketik ulang teks berikut</label>
			<div class="col-sm-8 col-sm-offset-2 captcha-img">
				<img src="http://placehold.it/128x80" class="center-block" alt="">
			</div>
			<div class="col-sm-8 col-sm-offset-2">
				<input type="text" id="captcha" class="form-control">
			</div>
		</div>
		<div class="form-group">
			<div class="col-sm-6 col-sm-offset-3">
				<button type="submit" class="btn btn-primary btn-block">
					Kirim
				</button>
			</div>
		</div>
	</div>
</form>
